---
layout: nil
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title><![CDATA[Caos Ordenado]]></title>
  <link href="http://caosordenado.com/feed.xml" rel="self"/>
  <link href="http://caosordenado.com/"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://caosordenado.com/</id>
  <author>
    <name><![CDATA[Eduardo Fernandes]]></name>
  </author>

  {% for post in site.posts limit: 20 %}
  <entry>
    <title type="html"><![CDATA[{{ post.title | cdata_escape }}]]></title>
    <link href="http://caosordenado.com{{ post.url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>http://caosordenado.com{{ post.id }}</id>
    <content type="html"><![CDATA[{{ post.content | expand_urls: site.url | cdata_escape }}]]></content>
  </entry>
  {% endfor %}
</feed>
